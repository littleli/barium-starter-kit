{
  "name": "barium-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for React spiced up with MobX state management",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" --exclude \"src/**/*.d.ts\"",
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --hot --inline --progress --colors",
    "build": "cross-env NODE_ENV=production webpack -p --progress --colors"
  },
  "keywords": [
    "typescript",
    "reactjs",
    "mobx",
    "jest"
  ],
  "author": "Ales Najmann <ales.najmann@gmail.com>",
  "license": "ISC",
  "jest": {
    "bail": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "scriptPreprocessor": "<rootDir>/config/preprocessor.js",
    "testRegex": "<rootDir>/src/.*\\.spec\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/fileMock.js",
      "^.+\\.(css|less|sass)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "@types/enzyme": "^2.5.37",
    "@types/jest": "^15.1.32",
    "@types/node": "^6.0.46",
    "@types/react": "^0.14.43",
    "@types/react-addons-test-utils": "^0.14.15",
    "@types/react-dom": "^0.14.18",
    "@types/react-router": "^2.0.38",
    "@types/whatwg-fetch": "0.0.32",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "copy-webpack-plugin": "4.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^16.0.2",
    "json-loader": "^0.5.4",
    "mobx": "^2.6.0",
    "mobx-react": "3.5.8",
    "mobx-react-devtools": "4.2.9",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^1.1.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.6.1",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^2.1.0",
    "tslint-loader": "^2.1.5",
    "tslint-react": "^1.1.0",
    "typescript": "^2.0.6",
    "typescript-formatter": "^4.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "2.13.1",
    "webpack-split-by-path": "0.1.0-beta.1",
    "whatwg-fetch": "^1.0.0",
    "zone.js": "^0.6.26"
  }
}
